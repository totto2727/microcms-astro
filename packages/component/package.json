{
  "name": "component",
  "volta": {
    "node": "18.16.1",
    "pnpm": "8.6.7"
  },
  "scripts": {
    "prebuild": "panda codegen",
    "watch": "panda watch",
    "type": "astro check",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint/.eslint-cache --fix .",
    "format": "prettier . --write --cache"
  },
  "type": "module",
  "files": [
    "index.ts",
    "src"
  ],
  "main": "./index.ts",
  "exports": {
    ".": {
      "types": "./index.ts",
      "import": "./index.ts",
      "require": "./index.ts"
    },
    "./page": {
      "types": "./src/page.ts",
      "import": "./src/page.ts",
      "require": "./src/page.ts"
    },
    "./layout": {
      "types": "./src/layout.ts",
      "import": "./src/layout.ts",
      "require": "./src/layout.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./index.ts"
      ],
      "page": [
        "./src/page.ts"
      ],
      "layout": [
        "./src/layout.ts"
      ]
    }
  },
  "dependencies": {
    "@astrojs/react": "^2.2.1",
    "@pandacss/dev": "^0.6.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "astro": "^2.8.1",
    "pandacss-config": "workspace:^",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
