{
  "name": "component",
  "volta": {
    "extends": "../../package.json"
  },
  "scripts": {
    "prebuild": "panda codegen",
    "watch": "panda watch",
    "type": "astro check",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint/.eslint-cache --fix .",
    "format": "prettier . --write --cache"
  },
  "type": "module",
  "files": [
    "index.ts",
    "src"
  ],
  "main": "./index.ts",
  "exports": {
    "./page/*": {
      "types": "./src/page/*",
      "import": "./src/page/*",
      "require": "./src/page/*"
    },
    "./layout/*": {
      "types": "./src/layout/*",
      "import": "./src/layout/*",
      "require": "./src/layout/*"
    }
  },
  "typesVersions": {
    "*": {
      "page/*": [
        "./src/page/*"
      ],
      "layout/*": [
        "./src/layout/*"
      ]
    }
  },
  "dependencies": {
    "@astrojs/react": "^2.2.1",
    "@pandacss/dev": "^0.6.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "astro": "^2.9.3",
    "pandacss-config": "workspace:^",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
