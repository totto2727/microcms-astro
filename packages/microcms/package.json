{
  "name": "microcms",
  "volta": {
    "extends": "../../package.json"
  },
  "scripts": {
    "type": "tsc",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint/.eslint-cache --fix .",
    "format": "prettier . --write --cache"
  },
  "type": "module",
  "exports": {
    ".": "./index.ts",
    "./query/*": "./src/query/*.ts"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./index.ts"
      ],
      "query/*": [
        "./src/query/*"
      ]
    }
  },
  "files": [
    "index.ts",
    "src"
  ],
  "dependencies": {
    "microcms-js-sdk": "^2.5.0",
    "result": "workspace:^"
  }
}
