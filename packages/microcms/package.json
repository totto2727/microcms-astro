{
  "name": "microcms",
  "volta": {
    "extends": "../../../package.json"
  },
  "scripts": {
    "type": "tsc",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint/.eslint-cache --fix .",
    "format": "prettier . --write --cache"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.ts",
      "import": "./index.ts",
      "require": "./index.ts"
    },
    "./query": {
      "types": "./src/query/*",
      "import": "./src/query/*",
      "require": "./src/query/*"
    }
  },
  "files": [
    "index.ts",
    "src"
  ],
  "dependencies": {
    "microcms-js-sdk": "https://github.com/totto2727-org/microcms-js-sdk",
    "result": "workspace:^"
  }
}
