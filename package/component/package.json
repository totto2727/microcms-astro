{
  "name": "component",
  "volta": {
    "extends": "../../package.json"
  },
  "scripts": {
    "prebuild": "panda codegen",
    "watch": "panda watch",
    "type": "astro check",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint/.eslint-cache --fix .",
    "format": "prettier . --write --cache **/*.{js,jsx,mjs,cjs,ts,tsx,json,md,html,css,scss,astro,svelte} --no-error-on-unmatched-pattern"
  },
  "type": "module",
  "files": [
    "index.ts",
    "src"
  ],
  "exports": {
    "./page/*": {
      "types": "./src/page/*",
      "import": "./src/page/*",
      "require": "./src/page/*"
    },
    "./layout/*": {
      "types": "./src/layout/*",
      "import": "./src/layout/*",
      "require": "./src/layout/*"
    }
  },
  "typesVersions": {
    "*": {
      "page/*": [
        "./src/page/*"
      ],
      "layout/*": [
        "./src/layout/*"
      ]
    }
  },
  "dependencies": {
    "@astrojs/react": "^2.2.1",
    "@pandacss/dev": "^0.9.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "astro": "^2.10.1",
    "date-fns": "^2.30.0",
    "microcms": "workspace:^",
    "pandacss-config": "workspace:^",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
