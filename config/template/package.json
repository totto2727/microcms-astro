{
  "name": "template",
  "volta": {
    "extends": "../../package.json"
  },
  "scripts": {
    "type": "tsc",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint/.eslint-cache --fix .",
    "format": "prettier . --write --cache **/*.{js,jsx,mjs,cjs,ts,tsx,json,md,html,css,scss,astro,svelte} --no-error-on-unmatched-pattern --ignore-path ../../.gitignore"
  },
  "type": "module",
  "files": [
    "index.ts",
    "src"
  ],
  "exports": {
    ".": "./index.ts"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./index.ts"
      ]
    }
  },
  "dependencies": {
    "@types/node": "^20.5.0",
    "ts-config": "workspace:^",
    "typescript": "^5.1.6"
  }
}
