{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "noEmit": false,
    "emitDeclarationOnly": true,
    "declaration": true,
    "incremental": true,
    "exactOptionalPropertyTypes": false,
    "resolveJsonModule": true,
    "allowJs": true
  }
}
