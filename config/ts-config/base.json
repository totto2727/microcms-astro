{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "nodenext",
    "noEmit": true,
    "exactOptionalPropertyTypes": false,
    "incremental": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "isolatedModules": true
  }
}
