{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    ".env.local"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "dev": {
      "dependsOn": [
        "^dev"
      ]
    },
    "check": {},
    "lint": {
      "outputs": [
        "./node_modules/.cache/eslint/.eslint-cache"
      ]
    },
    "format": {
      "outputs": [
        "./node_modules/.cache/prettier/.prettier-cache"
      ]
    },
    "static-analysis": {
      "dependsOn": [
        "check",
        "lint",
        "format"
      ],
      "outputs": [
        "./node_modules/.cache/prettier/.prettier-cache",
        "./node_modules/.cache/eslint/.eslint-cache"
      ]
    }
  }
}