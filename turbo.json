{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env", ".env.local"],
  "pipeline": {
    "prebuild": {
      "dependsOn": ["^prebuild"],
      "outputs": ["./node_modules/pandacss/**"]
    },
    "build": {
      "dependsOn": ["^build", "prebuild"],
      "outputs": ["dist/**"]
    },
    "start": {
      "dependsOn": ["build"]
    },
    "dev": {
      "dependsOn": ["prebuild", "^dev"]
    },
    "type": {
      "dependsOn": ["prebuild"],
      "outputs": [".ts-cache/**"]
    },
    "lint": {
      "dependsOn": ["type"],
      "outputs": ["./node_modules/.cache/eslint/.eslint-cache"]
    },
    "format": {
      "dependsOn": ["lint"],
      "outputs": ["./node_modules/.cache/prettier/.prettier-cache"]
    }
  }
}
